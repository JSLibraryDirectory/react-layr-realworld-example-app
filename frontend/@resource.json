{
  "@import": [
    "aws/s3-hosted-website#^0.1.0",
    "js/resource#^0.1.0"
  ],
  "customErrors": [
    {
      "errorCode": 404,
      "responseCode": 200,
      "responsePage": "index.html"
    }
  ],
  "dependencies": {
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react-router-dom": "^5.0.1"
  },
  "build": {
    "@type": "method",
    "@run": [
      "transpiler run",
      "bundler run",
      "copier run",
      "freezer run"
    ],
    "@listen": "@build"
  },
  "transpiler": {
    "@import": "js/transpiler#^0.1.15",
    "targets": {
      "chrome": "51",
      "safari": "10",
      "firefox": "54"
    },
    "format": "esm",
    "transformJSX": true
  },
  "bundler": {
    "@import": "js/bundler#^0.1.0",
    "entry": "./dist/esm/index.js",
    "target": "browser",
    "format": "iife",
    "globals": {
      "react": "React",
      "react-dom": "ReactDOM"
    }
  },
  "copier": {
    "@import": "tool/file-copier#^0.1.0",
    "sourceDirectory": "./"
  },
  "freezer": {
    "@import": "website/freezer#^0.1.0"
  }
}
