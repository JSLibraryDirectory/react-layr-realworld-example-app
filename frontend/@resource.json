{
  "@import": [
    "aws/s3-hosted-website#^0.1.0",
    "js/resource#^0.1.0"
  ],
  "customErrors": [
    {
      "errorCode": 404,
      "responseCode": 200,
      "responsePage": "index.html"
    }
  ],
  "dependencies": {
    "@liaison/react-liaison-realworld-example-app-shared": "^0.1.0",
    "@liaison/layer-http-client": "^0.1.0",
    "@liaison/liaison": "^0.1.0",
    "@liaison/react-integration": "^0.1.0",
    "prop-types": "^15.7.2"
  },
  "build": {
    "@type": "method",
    "@run": [
      "bundler run",
      "copier run",
      "freezer run"
    ],
    "@listen": "@build"
  },
  "bundler": {
    "@import": "js/bundler#^0.2.0",
    "entry": "./src/index.js",
    "targets": {
      "chrome": "51",
      "safari": "10",
      "firefox": "54"
    },
    "transpile": true,
    "format": "iife",
    "globals": {
      "react": "React",
      "react-dom": "ReactDOM"
    }
  },
  "copier": {
    "@import": "tool/file-copier#^0.1.0",
    "sourceDirectory": "./"
  },
  "freezer": {
    "@import": "website/freezer#^0.1.0"
  }
}
