{
  "@load": "./@resource.json",
  "domainName": "backend.react-liaison-realworld-example-app.liaison.dev",
  "memorySize": 1024,
  "timeout": 15,
  "endpointType": "regional",
  "aws": {
    "region": "us-west-2"
  },
  "main": "./dist/prod/index.js",
  "files": [
    "./dist/prod",
    "node_modules"
  ],
  "reinstallDependenciesWhileDeploying": true,
  "transpiler": {
    "destination": "./dist/prod"
  }
}
