{
  "@import": "1place/js-repository#^0.1.0",
  "backend": {
    "@include": "./backend"
  },
  "frontend": {
    "@include": "./frontend"
  },
  "shared": {
    "@include": "./shared"
  },
  "app": {
    "@import": "shell/concurrently#^0.1.0",
    "commands": [
      "./database/start.sh",
      "run ./backend start --@dev",
      "run ./frontend start --@dev"
    ],
    "run": {
      "@listen": "@start"
    }
  }
}
